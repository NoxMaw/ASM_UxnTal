( add.tal ) ( prints 4 5 5 2 to console )

( devices )
|10 @Console [ &vector $2 &read $1 &pad $5 &write $1 &error $1 ]

( macros )
%EMIT { .Console/write DEO }
%PRD { #30 ADD EMIT }
%NL { #0a EMIT }

( main )
|0100
	#04
	PRD
	NL
	#05
	PRD
	NL
	#0203
	ADD
	PRD
	NL
	#0301
	SUB
	PRD